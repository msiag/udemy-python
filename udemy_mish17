date = int(input("enter the date"))
month = int(input("enter the month"))

month_list_odd = [1,3,5,7,8,10,12]
month_list_even =[4,6,9,11]
feb = [2]
if month in month_list_odd:
    ans = (31 - date)
    print(ans)
elif month in month_list_even:
    ans = (30-date)
    print(ans)
else:
    ans= (28 - date)
    print(ans)
